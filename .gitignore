# Crashlytics generated file
crashlytics-build.properties

# Autogenerated Jetbrains Rider plugin files
.idea/
*.sln.iml

# .vscode
.vscode/

# Ignore Visual Studio Code project-specific files
.vscode/

# Ignore JetBrains Rider project-specific files
.idea/

# Ignore files from npm
node_modules/

# Windows image file caches
Thumbs.db

# Folder config file for macOS
.DS_Store
